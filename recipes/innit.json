      "name": "Preheat",
      "itemListElement": [
        {
          "text": "Preheat the oven to 425F."
        },
        {
          "text": "Line sheet pan with foil."
        }
      ]
    },
    {
      "name": "Sear Tofu",
      "itemListElement": [
        {
          "text": "Drain, pat dry \u0026 prepare tofu."
        },
        {
          "text": "Heat pan on high heat for 2 minutes."
        },
        {
          "text": "Cook for 7 min or until golden brown on all sides, seasoning half way."
        },
        {
          "text": "Remove from pan."
        }
      ]
    },
    {
      "name": "Bake Broccoli",
      "itemListElement": [
        {
          "text": "Toss broccoli with oil \u0026 salt."
        },
        {
          "text": "Bake for 22 minutes."
        }
      ]
    },
    {
      "name": "Simmer Carrots \u0026 Ginger",
      "itemListElement": [
        {
          "text": "Prepare ingredients."
        },
        {
          "text": "Pre-heat pan. Add all ingredients; cover with water."
        },
        {
          "text": "Cook until soft, about 5 minutes."
        }
      ]
    },
    {
      "name": "Blend Dressing Ingredients",
      "itemListElement": [
        {
          "text": "Transfer carrot-ginger mixture to blender."
        },
        {
          "text": "Add orange juice/zest, miso, sesame oil, honey, rice vinegar."
        },
        {
          "text": "Blend until smooth."
        },
        {
          "text": "Let cool in fridge for 10 minutes. Fold in chives \u0026 season."
        }
      ]
    },
    {
      "name": "Flavor Beets",
      "itemListElement": [
        {
          "text": "Toss beets in oil. Season with salt, pepper \u0026 parsley."
        }
      ]
