  "recipeIngredient": [
    "2 cups granulated sugar",
    "1 3/4 cup + 2 Tablespoons all-purpose flour",
    "3/4 cup unsweetened cocoa powder",
    "1 1/2 teaspoons baking powder",
    "1 1/2 teaspoons baking soda",
    "1 teaspoon salt",
    "2 large eggs",
    "1 cup buttermilk",
    "1/2 cup oil ((vegetable or canola oil))",
    "2 teaspoons vanilla extract",
    "1 cup boiling water (, or hot coffee)",
    "1/2 cup melted butter",
    "2/3 cup unsweetened cocoa powder",
    "3 cups powdered sugar",
    "1/3 cup milk",
    "1 teaspoon vanilla extract"
  ],
  "recipeInstructions": [
    {
      "text": "Heat oven to 350°F. Line two 8 inch round baking pans with parchment paper and then spray lightly with non-stick cooking spray. Set aside.",
      "url": "https://tastesbetterfromscratch.com/hersheys-perfectly-chocolate-chocolate-cake/#wprm-recipe-13824-step-0-0"
    },
    {
      "text": "Stir together sugar, flour, cocoa, baking powder, baking soda and salt in large bowl.",
      "url": "https://tastesbetterfromscratch.com/hersheys-perfectly-chocolate-chocolate-cake/#wprm-recipe-13824-step-0-1"
    },
    {
      "text": "Add eggs, milk, oil and vanilla and mix until well combined.",
      "url": "https://tastesbetterfromscratch.com/hersheys-perfectly-chocolate-chocolate-cake/#wprm-recipe-13824-step-0-2"
    },
    {
      "text": "Stir in boiling water (or hot coffee). Batter will be thin, but that's okay! Pour batter into prepared pans.",
      "url": "https://tastesbetterfromscratch.com/hersheys-perfectly-chocolate-chocolate-cake/#wprm-recipe-13824-step-0-3"
    },
    {
      "text": "Bake 30 to 35 minutes or until a toothpick inserted in the center of the cake comes out clean.",
      "url": "https://tastesbetterfromscratch.com/hersheys-perfectly-chocolate-chocolate-cake/#wprm-recipe-13824-step-0-4"
    },
    {
      "name": "Chocolate Frosting:",
      "itemListElement": [
        {
          "text": "Combine butter and cocoa powder.",
          "url": "https://tastesbetterfromscratch.com/hersheys-perfectly-chocolate-chocolate-cake/#wprm-recipe-13824-step-1-0"
        },
        {
          "text": "Add powdered sugar, milk, and vanilla extract. Mix for several minutes until smooth and fluffy.",
          "url": "https://tastesbetterfromscratch.com/hersheys-perfectly-chocolate-chocolate-cake/#wprm-recipe-13824-step-1-1"
        }
      ]
    },
    {
      "name": "Assemble:",
      "itemListElement": [
        {
          "text": "Place one cake layer on serving tray. Spread a generous amount of frosting on top. Add second cake layer. Frost top and sides of cake with remaining frosting.",
          "url": "https://tastesbetterfromscratch.com/hersheys-perfectly-chocolate-chocolate-cake/#wprm-recipe-13824-step-2-0"
        },
        {
          "text": "Cut and serve.",
          "url": "https://tastesbetterfromscratch.com/hersheys-perfectly-chocolate-chocolate-cake/#wprm-recipe-13824-step-2-1"
        }
      ]
    }
