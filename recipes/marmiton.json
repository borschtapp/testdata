  "recipeIngredient": [
    "350 g d'aubergine",
    "350 g de courgette",
    "350 g de poivron de couleur rouge et vert",
    "350 g d'oignon",
    "500 g de tomate bien mûres",
    "6 c.à.s d'huile d'olive",
    "1 brin de thym",
    "1 feuille de laurier",
    "poivre",
    "sel",
    "3 gousses d'ail"
  ],
  "recipeInstructions": [
    {
      "text": "Coupez les tomates pelées en quartiers"
    },
    {
      "text": "les aubergines et les courgettes en rondelles."
    },
    {
      "text": "Emincez les poivrons en lamelles"
    },
    {
      "text": "et l'oignon en rouelles."
    },
    {
      "text": "Chauffez 2 cuillères à soupe d'huile dans une poêle"
    },
    {
      "text": "et faites-y fondre les oignons et les poivrons."
    },
    {
      "text": "Lorsqu'ils sont tendres, ajoutez les tomates, l'ail haché, le thym et le laurier."
    },
    {
      "text": "Salez, poivrez et laissez mijoter doucement à couvert durant 45 minutes."
    },
    {
      "text": "Pendant ce temps, préparez les aubergines et les courgettes. Faites les cuire séparemment ou non dans l'huile d'olive pendant 15 minutes."
    },
    {
      "text": "Vérifiez la cuisson des légumes pour qu'ils ne soient plus fermes. Ajoutez les alors au mélange de tomates et prolongez la cuisson sur tout petit feu pendant 10 min."
    },
    {
      "text": "Salez et poivrez si besoin."
    }
  ],
